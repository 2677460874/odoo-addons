<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="tao_product_normal_form_view" model="ir.ui.view">
            <field name="name">tao_product.normal.form2</field>
            <field name="model">product.product</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="taoproduct.product_normal_form_view"/>
 			<field eval="50" name="priority"/>
             <field name="arch" type="xml">
             <xpath expr="//page[@string='PMC']" position="attributes">
	              <attribute name="groups">taoutil.res_group_pmc0</attribute>
             </xpath>
             <xpath expr="//page[@string='Sourcing']" position="attributes">
	              <attribute name="groups">taoutil.res_group_sourcing0,taoutil.res_group_marketing0,taoutil.res_group_rd1</attribute>
             </xpath>
            </field>
        </record>
    </data>
</openerp>
